{
  "manifest_version": 3,
  "name": "Snake Bot",
  "version": "1.0",
  "description": "Запускает instant_response_bot для игры змейка",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Snake Bot"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}